                <someValuesFrom rdf:resource="http://example.org/smart-tourism#CuisineType"/>
            </Restriction>
        </equivalentClass>
        <rdfs:comment xml:lang="en">Class representing restaurant.</rdfs:comment>
        <rdfs:label xml:lang="en">Restaurant</rdfs:label>
    </Class>
    


